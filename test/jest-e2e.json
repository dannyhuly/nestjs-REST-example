{
  "moduleFileExtensions": ["js", "json", "ts"],
  "rootDir": "../",
  "verbose": true,
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "collectCoverageFrom": [
    "src/**/*.(t|j)s"
  ],
  "collectCoverage": true,
  "coverageDirectory": "coverage/e2e"
}
